---
to: libs/<%=contextFileName%>/<%=timeLineFileName%>/src/lib/<%=commandFileName%>/<%=timeLineFileName%>-<%=commandFileName%>.command.ts
force: true
---
import { command, <%if (isStartOfEventStream) {%>Responding<%}%>Command } from '@ebd-connect/cqrs-framework';

@command('<%=ContextName%>.<%=TimeLineName%>.<%=CommandName%>')
export class <%=TimeLineName%><%=CommandName%> implements <%if (isStartOfEventStream) {%>Responding<%}%>Command {
  constructor(
<% command.facts.map( (item) => {%>    public readonly <%=h.changeCase.camel(item.name)%> : <%=item.dataType%>,
<% }) -%>  ) {}
<%if (isStartOfEventStream) {%>  $responseType!: { <%=eventStreamIdentifier%> : <%=eventStreamIdentifierType%> }<%}-%>
}
